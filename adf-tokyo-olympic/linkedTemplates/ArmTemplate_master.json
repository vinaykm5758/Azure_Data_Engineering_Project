{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-tokyo-olympic"
		},
		"LS_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeStorage'"
		},
		"AthletesHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/vinaykm5758/Azure_Data_Engineering_Project/main/Athletes.csv"
		},
		"LS_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tokyoolympicdastorage.dfs.core.windows.net/"
		},
		"LS_CoachesHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/vinaykm5758/Azure_Data_Engineering_Project/main/Coaches.csv"
		},
		"LS_EntriesGenderHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/vinaykm5758/Azure_Data_Engineering_Project/main/EntriesGender.csv"
		},
		"LS_MedalsHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/vinaykm5758/Azure_Data_Engineering_Project/main/Medals.csv"
		},
		"LS_TeamsHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/vinaykm5758/Azure_Data_Engineering_Project/main/Teams.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-tokyo-olympic_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('LS_AzureDataLakeStorage_accountKey')]"
					},
					"AthletesHTTP_properties_typeProperties_url": {
						"value": "[parameters('AthletesHTTP_properties_typeProperties_url')]"
					},
					"LS_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"LS_CoachesHTTP_properties_typeProperties_url": {
						"value": "[parameters('LS_CoachesHTTP_properties_typeProperties_url')]"
					},
					"LS_EntriesGenderHTTP_properties_typeProperties_url": {
						"value": "[parameters('LS_EntriesGenderHTTP_properties_typeProperties_url')]"
					},
					"LS_MedalsHTTP_properties_typeProperties_url": {
						"value": "[parameters('LS_MedalsHTTP_properties_typeProperties_url')]"
					},
					"LS_TeamsHTTP_properties_typeProperties_url": {
						"value": "[parameters('LS_TeamsHTTP_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}